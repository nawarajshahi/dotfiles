CheckHostIP yes
HashKnownHosts yes
Protocol 2
IdentitiesOnly yes

ServerAliveInterval 10

Host 192.168.122.*
  IdentityFile ~/.ssh/gpg_key.pub
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host github.com
  IdentityFile ~/.ssh/gpg_key.pub

Host cackling-evening-brook.stelfox.net
  IdentityFile ~/.ssh/gpg_key.pub
  Port 2200
  FingerprintHash=sha256
  VerifyHostKeyDNS=yes

