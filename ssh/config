  FingerprintHash=sha256
  VerifyHostKeyDNS=yes

Host mtun
  HostName singing-evening-road.stelfox.net
  IdentityFile ~/.ssh/gpg_key.pub
  Port 2200
  FingerprintHash=sha256
  VerifyHostKeyDNS=yes
  RemoteForward 127.0.0.1:8000 127.0.0.1:3000

Host web01
  HostName host.stelfox.net
  IdentityFile ~/.ssh/gpg_key.pub
  FingerprintHash=sha256
  VerifyHostKeyDNS=yes
  Port 2200

Host *.i
  IdentityFile ~/.ssh/gpg_key.pub
  ProxyCommand ssh -q singing-evening-road.stelfox.net "nc 127.0.0.1 $(echo %h | sed 's;\.i$;;')"

Host *.stelfox.net
  IdentityFile ~/.ssh/gpg_key.pub

Host 10.*
  IdentityFile ~/.ssh/gpg_key.pub

Host 127.0.0.1
  IdentityFile ~/.ssh/gpg_key.pub

# Begin Minim
Host *.minim.co
  IdentityFile ~/.ssh/gpg_key.pub

Host bastion.minim.co
  ControlMaster auto
  ControlPersist 5m
  IdentityFile ~/.ssh/gpg_key.pub
  User sstelfox

Host monitor.minim.co
  HostName 10.0.5.51
  IdentityFile ~/.ssh/gpg_key.pub
  LocalForward 127.0.0.1:3000 127.0.0.1:3000
  ProxyCommand ssh -W %h:%p -q bastion.minim.co

Host nginx.minim.co
  Hostname 10.0.1.160
  IdentityFile ~/.ssh/nginx.pem
  User ec2-user
  ProxyCommand ssh bastion.minim.co -W %h:%p

